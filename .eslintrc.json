{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": ["prettier", "eslint:recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-trailing-spaces": 0
    }
}
